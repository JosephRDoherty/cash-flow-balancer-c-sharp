<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Joe's Laboratory</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/cash_flow_balancer_c_sharp.styles.css" asp-append-version="true" />
    <link href='https://fonts.googleapis.com/css?family=Open Sans' rel='stylesheet'>
    <link href='https://fonts.googleapis.com/css?family=Merriweather' rel='stylesheet'>
    <script src="https://kit.fontawesome.com/86a01f560a.js" crossorigin="anonymous"></script>
</head>
<body>
    <header>
        <nav class="navBar">
            <h3>J|Labs<span id="darkModeBtn"><i class="fa-solid fa-moon"></i></span></h3>
            <ul>
                <li><a asp-page="/Index">Home</a></li>
                <li><a asp-page="/Privacy">Privacy</a></li>
                <li><a asp-page="/FireMountain">Fire Mountain Gems</a></li>
                <li><a asp-page="/CashBalancerView">Cash Flow Balancer</a></li>
            </ul>
        </nav>
    </header>
    <div id="docBody">
            @RenderBody()
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Joe's Laboratory - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    
    

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>